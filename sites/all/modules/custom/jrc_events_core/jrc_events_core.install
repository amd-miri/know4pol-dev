<?php

/**
 * @file
 * Install the feature events.
 */

/**
 * Add block to the context.
 */
function jrc_events_core_enable() {
  drupal_set_message(t('JRC Events feature is now active on your site.'));
}

/**
 * Remove block from the context.
 */
function jrc_events_core_disable() {
  drupal_set_message(t('JRC Events feature is now disabled on your site.'));
}
